<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.meijinzhi.apply.dao.ResumeInfoMapper">
    <select id="selectByUserId" resultType="com.meijinzhi.apply.entity.ResumeInfo">
        select ri.*
        from resume_info ri
        where user_id = #{userId}
    </select>
</mapper>