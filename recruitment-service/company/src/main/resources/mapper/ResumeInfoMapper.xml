<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.meijinzhi.company.dao.ResumeInfoMapper">
    <select id="selectByUser" resultType="com.meijinzhi.company.entity.ResumeInfo">
        select *
        from resume_info
        where user_id = #{userId}
    </select>
</mapper>